{
  "name": "@kyber.network/reserve-sdk",
  "version": "0.0.4",
  "description": "JavaScript Reserve SDK",
  "main": "./dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --require babel-core/register --require babel-polyfill --timeout 10s",
    "format": "prettier-standard 'src/**/*.js' 'test/**/*.js'",
    "lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "doc": "esdoc",
    "build": "BABEL_ENV=production babel src --out-dir dist",
    "prepare": "npm run build"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyberNetwork/reserve-sdk.js.git"
  },
  "author": "Diep Pham <diep@kyber.network>",
  "contributors": [
    "Liem D. Nguyen <liem@kyber.network>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyberNetwork/reserve-sdk.js/issues"
  },
  "homepage": "https://github.com/KyberNetwork/reserve-sdk.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "ganache-cli": "^6.1.8",
    "mocha": "^5.2.0",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^7.2.1",
    "web3": "^1.0.0-beta.35"
  }
}
