{
  "name": "reserve-sdk",
  "version": "0.0.1",
  "description": "JavaScript Reserve SDK",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --require babel-core/register --require babel-polyfill test/base_contract.test.js",
    "format": "prettier-standard 'src/**/*.js' 'test/**/*.js'",
    "lint": "standard",
    "doc": "jsdoc src",
    "build": "BABEL_ENV=production babel src --out-dir lib"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyberNetwork/reserve-sdk.js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyberNetwork/reserve-sdk.js/issues"
  },
  "homepage": "https://github.com/KyberNetwork/reserve-sdk.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "ganache-cli": "^6.1.8",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "web3": "^1.0.0-beta.35"
  }
}
